#!/usr/bin/env python
import argparse

def process():
	"""
	Main entry point for command line usage.
	"""
	parser = argparse.ArgumentParser(description="Generate for a single W3C TR document.")
	parser.add_argument("URI", help="URI of the original ")
	args = parser.parse_args()

	from rp2epub import DocToEpub
	DocToEpub(args.URI).process()

if __name__ == '__main__':
	process()
